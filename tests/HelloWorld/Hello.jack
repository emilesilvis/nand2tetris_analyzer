class Hello {
   field String name;
   field int number;

    constructor Hello new(String aname) {
        let name = aname;
        let number = 7 + (7 * 3);

        return this;
    } 

   method void hello(String apunctuation) {
      var String punctuation;

      let punctuation = apunctuation;

      do Output.printString("Hello ");
      do Output.printString(name);
      do Output.printString(punctuation);
      do Output.println();
      do Output.printInt(number);
      return;
   }
}
